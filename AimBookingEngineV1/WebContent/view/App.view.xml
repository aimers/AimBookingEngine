<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:u="sap.ui.unified" controllerName="sap.ui.medApp.view.App"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<u:Shell id="myShell" class="mainShell"
		icon="assets/img/logo.png">
		<u:headItems>
			<u:ShellHeadItem tooltip="Home" icon="sap-icon://home"
				press="handelHomeBtn" />
		</u:headItems>
		<u:headEndItems>
			<u:ShellHeadItem icon="sap-icon://favorite" tooltip="Logoff"
				press="handleLogoffPress" />
		</u:headEndItems>
		<u:headEndItems>
			<u:ShellHeadItem icon="sap-icon://customer-history"
				tooltip="Logoff" press="handleLogoffPress" />
		</u:headEndItems>
		<u:headEndItems>
			<u:ShellHeadItem icon="sap-icon://log" tooltip="Logoff"
				press="handleLogoffPress" />
		</u:headEndItems>
		<u:user>
			<u:ShellHeadUserItem image="sap-icon://person-placeholder"
				username="{/fullName}" press="handleUserItemPressed" />
		</u:user>
	</u:Shell>
</core:View>